# ETag
FileETag MTime Size
<IfModule mod_headers.c>
    FileETag none
</IfModule>

# Headers for minimized JavaScript / CSS
<FilesMatch "\.(merge|min|gz)(\.gz)?\.(js|css)">
    <IfModule mod_expires.c>
        ExpiresDefault "access plus 1 year"
    </IfModule>

    <IfModule mod_headers.c>
        Header append Cache-Control "public"
    </IfModule>
</FilesMatch>